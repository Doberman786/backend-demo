{
  "name": "shared-data-tables",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/data/tables/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/shared/data/tables",
        "main": "libs/shared/data/tables/src/index.ts",
        "tsConfig": "libs/shared/data/tables/tsconfig.lib.json",
        "assets": ["libs/shared/data/tables/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/shared/data/tables/**/*.ts"]
      }
    }
  },
  "tags": []
}
