{
  "name": "shared-data-entities",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/data/entities/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/shared/data/entities",
        "main": "libs/shared/data/entities/src/index.ts",
        "tsConfig": "libs/shared/data/entities/tsconfig.lib.json",
        "assets": ["libs/shared/data/entities/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/shared/data/entities/**/*.ts"]
      }
    }
  },
  "tags": []
}
